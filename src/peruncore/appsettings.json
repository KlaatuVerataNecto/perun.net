{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "ConnectionStrings": {
    "MySQLDatabase": "server=localhost;userid=perun;pwd=perun;port=3306;database=perun.net;sslmode=none;"
  },
  "AppSettings": {
    "GoogleClientId": "clientid",
    "GoogleClientSecret": "secret"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Literate" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "LiterateConsole" },
      {
        "Name": "File",
        "Args": { "path": "log\\peruncore.log" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Sample"
    }
  }
}
