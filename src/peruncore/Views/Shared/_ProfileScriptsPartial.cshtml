<environment names="Development">
    <script src="~/lib/cropperjs/dist/cropper.js" asp-append-version="true"></script>
    <script src="~/lib/cropit/dist/jquery.cropit.js" asp-append-version="true"></script>
    <script src="~/js/pages/profile.js" asp-append-version="true"></script>
</environment>
<environment names="Staging,Production">
        <script src="https://cdnjs.cloudflare.com/ajax/libs/cropperjs/1.2.1/cropper.min.js"
            asp-fallback-src="~/lib/cropperjs/dist/cropper.min.js"
            crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/cropit/0.5.1/jquery.cropit.min.js"
            asp-fallback-src="~/lib/cropit/dist/jquery.cropit.js"        
            crossorigin="anonymous"></script>
</environment>